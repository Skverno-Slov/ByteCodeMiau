<Page x:Class="KnowYourHome.Pages.MonumentsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:KnowYourHome.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MonumentsPage">

    <ScrollViewer Width="800" Height="450" Visibility="Visible" Loaded="Page_Loaded">
        <StackPanel>
            <Button Content="Назад" HorizontalAlignment="Left" VerticalAlignment="Top" Width="80" Margin="10"
            Command="{Binding NavigateBackCommand}"/>
            <StackPanel x:Name="MonumentsStackPanel"/>
        </StackPanel>
    </ScrollViewer>
</Page>
